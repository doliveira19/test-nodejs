{
  "name": "test-nodejs",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --no-notify --exit-child src/index.ts",
    "prebuild": "rm -rf ./build",
    "build": "cross-env NODE_ENV=prod ttsc --p ./tsconfig.json",
    "postinstall": "cp -n envs/.env.local .env || true;",
    "api-doc:bundler": " redocly bundle src/api-docs/swagger.json --output src/api-docs/openapi.json --ext json;",
    "test:dev": "jest -i --passWithNoTests --forceExit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@redocly/cli": "^1.5.0",
    "cacheman-redis": "^2.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "redis-promisify": "^0.1.2",
    "swagger-ui-express": "^5.0.0",
    "typescript-transform-paths": "^3.4.6",
    "zod": "^3.22.4"
